[{"id":"1112144","label":"DATEREF","properties":{"authority":"tla"},"links":[{"type":"BEGIN","follow":null,"target":1088524},{"type":"END","follow":null,"target":1088521}],"primary":false},{"id":"958351","label":"TRANSLATION","properties":{"language":"en","text":"And after 5 years locusts arose in that district, like sands of the sea, and ruined the earth."},"links":[{"type":"END","follow":null,"target":1088490},{"type":"BEGIN","follow":null,"target":1088525}],"primary":false},{"id":"951234","label":"COMMENT","properties":{"by":"tla","text":"This is a comment inserted for testing purposes."},"links":[{"type":"END","follow":null,"target":1088720},{"type":"BEGIN","follow":null,"target":1088643}],"primary":false},{"id":"958319","label":"TRANSLATION","properties":{"language":"en","text":"and nothing else was built up to the time of today."},"links":[{"type":"END","follow":null,"target":1023057},{"type":"BEGIN","follow":null,"target":1088406}],"primary":false},{"id":"1101721","label":"TITLE","properties":{"language":"hy","text":"401 թ. (2 Ապրիլ 952–1 Ապրիլ 953)"},"links":[{"type":"TITLED","follow":null,"target":1019321}],"primary":true},{"id":"985990","label":"TRANSLATION","properties":{"language":"en","text":"But in the southern land, in the country of the Muslims, there was great affliction, and most of all in Mesopotamia."},"links":[{"type":"END","follow":null,"target":1088691},{"type":"BEGIN","follow":null,"target":1088754}],"primary":false},{"id":"1156635","label":"TRANSLATION","properties":{"language":"en","text":"Now in those days and in the year 401 there was a severe famine in many places."},"links":[{"type":"END","follow":null,"target":1088798},{"type":"BEGIN","follow":null,"target":1088791}],"primary":false},{"id":"958373","label":"TRANSLATION","properties":{"language":"en","text":"And from the harshness of the famine anxiety and trembling arose in many places,"},"links":[{"type":"BEGIN","follow":null,"target":1088688},{"type":"END","follow":null,"target":1088665}],"primary":false},{"id":"1156637","label":"TRANSLATION","properties":{"language":"en","text":"and in the renowned mother city of Edessa,"},"links":[{"type":"BEGIN","follow":null,"target":1088664},{"type":"END","follow":null,"target":1088659}],"primary":false},{"id":"958370","label":"TRANSLATION","properties":{"language":"en","text":"and innumerable Christians died on account of the severity of the famine."},"links":[{"type":"END","follow":null,"target":1088526},{"type":"BEGIN","follow":null,"target":1088555}],"primary":false},{"id":"1110686","label":"DATING","properties":{"internal":true,"authority":"tla"},"links":[{"type":"END","follow":null,"target":1088526},{"type":"BEGIN","follow":null,"target":1088791}],"primary":false},{"id":"1095071","label":"TITLE","properties":{"language":"en","text":"The year 401 (2 April 952–1 April 953)"},"links":[{"type":"TITLED","follow":null,"target":1019321}],"primary":true},{"id":"1112127","label":"DATEREF","properties":{"authority":"tla"},"links":[{"type":"END","follow":null,"target":1088774},{"type":"BEGIN","follow":null,"target":1088776}],"primary":false},{"id":"958332","label":"TRANSLATION","properties":{"language":"en","text":"The famine intensified more and more,"},"links":[{"type":"BEGIN","follow":null,"target":1088484},{"type":"END","follow":null,"target":1088464}],"primary":false},{"id":"1153602","label":"PERSONREF","properties":{"authority":"tla"},"links":[{"type":"BEGIN","follow":null,"target":1088717},{"type":"END","follow":null,"target":1088717}],"primary":false},{"id":"958394","label":"TRANSLATION","properties":{"language":"en","text":"Many villages and districts became depopulated,"},"links":[{"type":"END","follow":null,"target":1027115},{"type":"BEGIN","follow":null,"target":1088372}],"primary":false},{"id":"958331","label":"TRANSLATION","properties":{"language":"en","text":"and many people died in the land of the Muslims."},"links":[{"type":"END","follow":null,"target":1088575},{"type":"BEGIN","follow":null,"target":1088593}],"primary":false},{"id":"958392","label":"TRANSLATION","properties":{"language":"en","text":"And the famine endured in that country for 5 years and there was immense carnage on account of that famine,"},"links":[{"type":"BEGIN","follow":null,"target":1088634},{"type":"END","follow":null,"target":1088807}],"primary":false},{"id":"958393","label":"TRANSLATION","properties":{"language":"en","text":"and many, growing enraged, savagely and mercilessly fell upon and ate each other."},"links":[{"type":"END","follow":null,"target":1088444},{"type":"BEGIN","follow":null,"target":1088461}],"primary":false},{"id":"1112074","label":"PLACEREF","properties":{"authority":"tla"},"links":[{"type":"BEGIN","follow":null,"target":1088695},{"type":"END","follow":null,"target":1088695}],"primary":false},{"id":"985588","label":"PLACEREF","properties":{"authority":"tla"},"links":[{"type":"END","follow":null,"target":1088648},{"type":"BEGIN","follow":null,"target":1088648}],"primary":false},{"id":"1112236","label":"PERSONREF","properties":{"authority":"tla"},"links":[{"type":"BEGIN","follow":null,"target":1088717},{"type":"END","follow":null,"target":1088717}],"primary":false},{"id":"958354","label":"TRANSLATION","properties":{"language":"en","text":"The princes and nobility were feeding themselves with grains and berries and there was a great passage of animals."},"links":[{"type":"END","follow":null,"target":1088371},{"type":"BEGIN","follow":null,"target":1088420}],"primary":false},{"id":"1238799","label":"DATING","properties":{"internal":false},"links":[{"type":"END","follow":null,"target":1023057},{"type":"BEGIN","follow":null,"target":1088525}],"primary":false},{"id":"958352","label":"TRANSLATION","properties":{"language":"en","text":"which was founded by Tigran the Armenian king."},"links":[{"type":"BEGIN","follow":null,"target":1088643},{"type":"END","follow":null,"target":1088635}],"primary":false}]